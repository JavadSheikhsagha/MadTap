<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="ir.nikgostarr.madtap">

    <uses-permission android:name="android.permission.WAKE_LOCK" />


    <application
        android:name="ir.nikgostarr.madtap.pack.MyAppClass"
        android:allowBackup="true"
        android:icon="@drawable/app_logo1"
        android:label="@string/app_name"
        android:roundIcon="@drawable/app_logo1"
        android:supportsRtl="true"
        android:theme="@style/Theme.ClickFaster">

        <activity
            android:screenOrientation="portrait"
            android:name="ir.nikgostarr.madtap.ui.MainActivity">

            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>

        <activity
            android:screenOrientation="portrait"
            android:name="ir.nikgostarr.madtap.ui.GameActivity">
        </activity>

    </application>

</manifest>